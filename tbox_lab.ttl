@prefix pub: <http://www.example.org/publication#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

pub:Authors a rdfs:Class ;
    rdfs:subClassOf pub:Scientist .

pub:Conference a rdfs:Class ;
    rdfs:subClassOf pub:Event .

pub:Conferencechair a rdfs:Class ;
    rdfs:subClassOf pub:Scientist .

pub:Event a rdfs:Class .

pub:JournalEditor a rdfs:Class ;
    rdfs:subClassOf pub:Scientist .

pub:Publication a rdfs:Class .

pub:RevelantAuthors a rdfs:Class ;
    rdfs:subClassOf pub:Authors .

pub:Review a rdfs:Class .

pub:Reviewer a rdfs:Class ;
    rdfs:subClassOf pub:RevelantAuthors .

pub:Scientist a rdfs:Class .

pub:Topic a rdfs:Class .

pub:Workshop a rdfs:Class ;
    rdfs:subClassOf pub:Event .

pub:abstract a rdf:Property ;
    rdfs:domain pub:Publication ;
    rdfs:range xsd:string .

pub:assignReviewersCCH a rdf:Property ;
    rdfs:domain pub:Conferencechair ;
    rdfs:range pub:Reviewer .

pub:assignReviewersJE a rdf:Property ;
    rdfs:domain pub:JournalEditor ;
    rdfs:range pub:Reviewer .

pub:cites a rdf:Property ;
    rdfs:domain pub:Publication ;
    rdfs:range pub:Publication .

pub:city a rdf:Property ;
    rdfs:domain pub:Event ;
    rdfs:range xsd:string .

pub:containsEvent a rdf:Property ;
    rdfs:domain pub:Event ;
    rdfs:range pub:Publication .

pub:containsJournal a rdf:Property ;
    rdfs:domain pub:Journal ;
    rdfs:range pub:Publication .

pub:discuss a rdf:Property ;
    rdfs:domain pub:Publication ;
    rdfs:range pub:Topic .

pub:editionAssignedCCH a rdf:Property ;
    rdfs:domain pub:JournalEditor ;
    rdfs:range pub:Journal .

pub:editionAssignedJE a rdf:Property ;
    rdfs:domain pub:Conferencechair ;
    rdfs:range pub:Event .

pub:name a rdf:Property ;
    rdfs:domain pub:Scientist ;
    rdfs:range xsd:string .

pub:proceedingID a rdf:Property ;
    rdfs:domain pub:Event ;
    rdfs:range xsd:string .

pub:reviews a rdf:Property ;
    rdfs:domain pub:Reviewer ;
    rdfs:range pub:Publication .

pub:title a rdf:Property ;
    rdfs:domain pub:Publication ;
    rdfs:range xsd:string .

pub:volume a rdf:Property ;
    rdfs:domain pub:Journal ;
    rdfs:range xsd:string .

pub:writes a rdf:Property ;
    rdfs:domain pub:Authors ;
    rdfs:range pub:Publication .

pub:year a rdf:Property ;
    rdfs:domain pub:Event ;
    rdfs:range xsd:string .

pub:yearJournal a rdf:Property ;
    rdfs:domain pub:Journal ;
    rdfs:range xsd:string .


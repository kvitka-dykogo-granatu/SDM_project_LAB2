@prefix pub: <http://www.example.org/publication#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

pub:Author a rdfs:Class .

pub:ConferenceChair a rdfs:Class ;
    rdfs:subClassOf pub:Editor .

pub:Editor a rdfs:Class ;
    rdfs:subClassOf pub:ReviewerBoard .

pub:Event a rdfs:Class .

pub:JournalEditor a rdfs:Class ;
    rdfs:subClassOf pub:Editor .

pub:Publication a rdfs:Class .

pub:Review a rdfs:Class .

pub:Reviewer a rdfs:Class ;
    rdfs:subClassOf pub:Author .

pub:ReviewerBoard a rdfs:Class ;
    rdfs:subClassOf pub:Reviewer .

pub:Topic a rdfs:Class .

pub:abstract a rdf:Property ;
    rdfs:domain pub:Paper,
        pub:Publication ;
    rdfs:range xsd:string .

pub:assignedBy a rdf:Property ;
    rdfs:domain pub:Review ;
    rdfs:range pub:Editor .

pub:cites a rdf:Property ;
    rdfs:domain pub:Paper ;
    rdfs:range pub:Paper .

pub:city a rdf:Property ;
    rdfs:domain pub:Proceeding,
        pub:Volume ;
    rdfs:range xsd:string .

pub:comment a rdf:Property ;
    rdfs:domain pub:Review ;
    rdfs:range xsd:string .

pub:decision a rdf:Property ;
    rdfs:domain pub:Review ;
    rdfs:range xsd:string .

pub:hIndex a rdf:Property ;
    rdfs:domain pub:Author ;
    rdfs:range xsd:int .

pub:hasTopic a rdf:Property ;
    rdfs:domain pub:Paper,
        pub:Publication ;
    rdfs:range pub:Topic .

pub:name a rdf:Property ;
    rdfs:domain pub:Author ;
    rdfs:range xsd:string .

pub:presentedIn a rdf:Property ;
    rdfs:domain pub:Publication ;
    rdfs:range pub:Proceeding .

pub:proceedingBelongsToEvent a rdf:Property ;
    rdfs:domain pub:Proceeding ;
    rdfs:range pub:Event .

pub:publishedIn a rdf:Property ;
    rdfs:domain pub:Publication ;
    rdfs:range pub:Volume .

pub:reviewedPaper a rdf:Property ;
    rdfs:domain pub:Review ;
    rdfs:range pub:Paper .

pub:reviews a rdf:Property ;
    rdfs:domain pub:Reviewer ;
    rdfs:range pub:Publication,
        pub:Reviewed .

pub:title a rdf:Property ;
    rdfs:domain pub:Paper,
        pub:Publication ;
    rdfs:range xsd:string .

pub:volumeBelongsToJournal a rdf:Property ;
    rdfs:domain pub:Volume ;
    rdfs:range pub:Journal .

pub:writes a rdf:Property ;
    rdfs:domain pub:Author ;
    rdfs:range pub:Paper,
        pub:Publication .

pub:year a rdf:Property ;
    rdfs:domain pub:Proceeding,
        pub:Volume ;
    rdfs:range xsd:int .


@prefix pub: <http://www.example.org/publication#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

pub:Authors a rdfs:Class .

pub:Event a rdfs:Class .

pub:Publication a rdfs:Class .

pub:Review a rdfs:Class .

pub:Scientist a rdfs:Class .

pub:Topic a rdfs:Class .

pub:Abstract a rdf:Property ;
    rdfs:domain pub:Paper ;
    rdfs:range xsd:string .

pub:Conference rdfs:subClassOf pub:Event .

pub:Conferencechair rdfs:subClassOf pub:Scientist .

pub:Has_topic a rdf:Property ;
    rdfs:domain pub:Paper ;
    rdfs:range pub:Topic .

pub:JournalEditor rdfs:subClassOf pub:Scientist .

pub:Title a rdf:Property ;
    rdfs:domain pub:Paper ;
    rdfs:range xsd:string .

pub:Workshop rdfs:subClassOf pub:Event .

pub:belongs_to a rdf:Property ;
    rdfs:domain pub:Proceeding,
        pub:Volume ;
    rdfs:range pub:Event,
        pub:Journal .

pub:city rdfs:domain pub:Proceeding,
        pub:Volume ;
    rdfs:range pub:string .

pub:has_year a rdf:Property .

pub:presented_in a rdf:Property ;
    rdfs:domain pub:Publication ;
    rdfs:range pub:Proceeding .

pub:published_in a rdf:Property ;
    rdfs:domain pub:Publication ;
    rdfs:range pub:Volume .

pub:review a rdf:Property ;
    rdfs:domain pub:Publication ;
    rdfs:range pub:Review .

pub:reviewedPaper a rdf:Property ;
    rdfs:domain pub:Review ;
    rdfs:range pub:Paper .

pub:reviewer a rdf:Property ;
    rdfs:domain pub:Review ;
    rdfs:range pub:Reviewer .

pub:year rdfs:domain pub:Proceeding,
        pub:Volume ;
    rdfs:range pub:int .

pub:RevelantAuthors rdfs:subClassOf pub:Authors .

pub:Reviewer rdfs:subClassOf pub:RevelantAuthors .

